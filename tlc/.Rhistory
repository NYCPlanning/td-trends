df=df %>%
mutate(Date=as.Date(paste0(YearMonth,'-01'),'%Y-%m-%d'))
dfcolors=c('Yellow Taxi'='rgba(255,158,74,0.8)',
'Green Taxi'='rgba(103,191,92,0.8)',
'For-Hire Vehicle'='rgba(114,158,206,0.8')
p=plot_ly()
for (i in c('For-Hire Vehicle','Yellow Taxi','Green Taxi')){
p=p %>%
add_trace(name=i,
type='scatter',
mode='none',
x=df[['Date']],
y=df[[i]],
showlegend=T,
stackgroup='one',
groupnorm='',
orientation='v',
fill='tonexty',
fillcolor=dfcolors[i],
hovertext=paste0('<b>',i,': </b>',format(df[[i]],trim=T,big.mark=',')),
hoverinfo='text')
}
